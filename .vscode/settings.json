{
    "javascript.validate.enable": false,
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/node_modules": true,
      "**/elm-stuff": true,
      "**/*.styl.d.ts": true,
      // "**/*.less.d.ts": true,
      //"**/dist": true
    },
    "prettier.disableLanguages": ["javascript", "jsx"],
    "prettier.ignorePath": ".*",
    "editor.tabSize": 2,
    "editor.formatOnPaste": false,
    "cSpell.words": [
      "Prelander",
      "affid",
      "checkperiod",
      "gamezones",
      "jmuffp",
      "relt",
      "shortcode",
      "xaid",
      "xcid"
    ],
  }