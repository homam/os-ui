{
    "javascript.validate.enable": false,
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/node_modules": true,
      "**/elm-stuff": true,
      "**/*.styl.d.ts": true,
